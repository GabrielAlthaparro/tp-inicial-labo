MDScreen:
    name:"loginTouch"
    FloatLayout:

        canvas:
            Color:
                rgba:(249/255,196/255,66/255, 1)
            Rectangle:
                pos:self.pos
                size:self.size
            Color:
                rgba:(221/255,218/255,213/255, 1)
            RoundedRectangle:
                size:self.width,self.height/3
                pos:0,self.height-150
                radius:[10,10,10,10]






        Image:
            source: "assets/fingerprint.png"
            pos_hint:{'center_x': 0.5, 'center_y': 0.20}
            size_hint:0.4,0.4

        Label:
            text:"Ingrese Con Su Huella Dactilar"
            haling:"center"
            theme_text_color: "Custom"
            text_color: 1,1,1,1
            font_size:"20sp"
            role:"small"
            pos_hint: {"center_x": .50, "center_y": .45}
            font_name:"BMont"

        Image:
            source: "assets/logo.png"


            pos_hint:{'center_x': 0.5, 'center_y': 0.9}
            size_hint:0.8,0.8





